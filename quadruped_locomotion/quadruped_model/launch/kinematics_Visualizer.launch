<launch>
  <arg
    name="model" />
  <arg
    name="gui"
    default="True" />
  <arg name="kinect_enabled" default="true"/>
  <arg name="robot_namespace" default="/"/>
  <arg name="DEBUG" default="false"/>  <!--This parameter refer to the robot is hang or not in the gazebo world -->
  <!-- <param
    name="robot_description"
    textfile="$(find simpledog)/urdf/quadruped_model.urdf" /> -->

  <param name="robot_description" command="$(find xacro)/xacro '$(find simpledog)/urdf/laikago/my_laika.xacro'
  --inorder
  robot_namespace:=$(arg robot_namespace)
  kinect_enabled:=$(arg kinect_enabled)
  DEBUG:=$(arg DEBUG)"/>
  <param
    name="use_gui"
    value="$(arg gui)" />

    <node name="send_joint_angle" pkg="quadruped_model" type="kinematics_visualizer_node" />


  <node name="robot_state_publisher" pkg="robot_state_publisher" type="state_publisher">
    <param name= "ignore_timestamp" type= "bool" value= "true"/>
    <!-- <remap from="/joint_states" to="/test_joint_states"/> -->
  </node>

  <node
    name="rviz"
    pkg="rviz"
    type="rviz"
    args="-d $(find simpledog)/config/kinematics_Visualizer.rviz"
    output="screen"/>


<!-- <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher" /> -->

</launch>
